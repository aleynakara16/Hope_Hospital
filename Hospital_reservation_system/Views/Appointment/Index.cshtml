@model IEnumerable<Hospital_reservation_system.Models.AppointmentViewModel>

@{
    ViewData["Title"] = "Randevularım";
}

<h1>@ViewData["Title"]</h1>


<table class="table">
    <thead>
    <th>User ID</th>
    <th>Selected Doctor ID</th>
    <th>Policlinic ID</th>
    <th>Date</th>
    <th>Time</th>
    </thead>
    <tbody>
        @foreach (var AppointmentViewModel in Model)
        {
            <tr>
                <td>@AppointmentViewModel.currentUserID</td>
                <td>@AppointmentViewModel.selecktedDoctorID</td>
                <td>@AppointmentViewModel.policlinicID</td>
                <td>@AppointmentViewModel.Date</td>
                <td>@AppointmentViewModel.Time</td>
            </tr>
        }
    </tbody>
</table>
